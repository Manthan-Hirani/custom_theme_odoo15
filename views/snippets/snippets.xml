<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="custom_snippets" inherit_id="website.snippets" name="Custom Snippets">
        <xpath expr="//div[@id='snippet_structure']" position="before">
            <div id="snippet_structure" class="o_panel">
                <div class="o_panel_header">Custom Theme
                    <div class="o_panel_body">
                        <t t-snippet="custom_theme.01"
                           t-thumbnail="/custom_theme/static/src/img/snippets_thumbs/s_about.jpg"/>
                        <t t-snippet="custom_theme.02"
                           t-thumbnail="/custom_theme/static/src/img/snippets_thumbs/s_about.jpg"/>
                        <t t-snippet="custom_theme.03"
                           t-thumbnail="/custom_theme/static/src/img/snippets_thumbs/s_about.jpg"/>
                        <t t-snippet="custom_theme.04"/>
                        <t t-snippet="custom_theme.05"/>
                        <t t-snippet="custom_theme.06"/>
                        <t t-snippet="custom_theme.07"/>
                        <t t-snippet="custom_theme.08"/>
                        <t t-snippet="custom_theme.10"/>
                        <t t-snippet="custom_theme.swiper_02"/>
                    </div>
                </div>
            </div>
        </xpath>
    </template>
</odoo>